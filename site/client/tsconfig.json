{
  "extends": "../../base.tsconfig.jsonc",
  "compilerOptions": {
    "composite": true,
    "lib": [ "ESNext", "DOM", "DOM.Iterable" ],
    "moduleDetection": "force",
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-native",
    "rootDir": ".",
    "declaration": true,
    "emitDeclarationOnly": true,
    "outDir": "out",
    "moduleResolution": "Node",
  },
  "exclude": [
    "types.d.ts",
    "./**/*.d.ts",
  ],
}
